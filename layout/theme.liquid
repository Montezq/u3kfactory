<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    <script src="{{ 'constants.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'pubsub.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'global.js' | asset_url }}" defer="defer"></script>
    {%- if settings.animations_reveal_on_scroll -%}
      <script src="{{ 'animations.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {{ content_for_header }}
    {% style %}
      @font-face {
          font-family: 'Helvetica ThinCond';
          src: url("{{ 'HelveticaNeue-ThinCond.eot' | file_url }}");
          src: url("{{ 'HelveticaNeue-ThinCond.eot#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'HelveticaNeue-ThinCond.woff2' | file_url }}") format('woff2'),
              url("{{ 'HelveticaNeue-ThinCond.woff' | file_url }}") format('woff'),
              url("{{ 'HelveticaNeue-ThinCond.ttf' | file_url }}") format('truetype'),
              url("{{ 'HelveticaNeue-ThinCond.svg#HelveticaNeue-ThinCond' | file_url }}") format('svg');
          font-display: swap;
      }

      @font-face {
          font-family: 'Helvetica ThinCondObl';
          src: url("{{ 'HelveticaNeue-ThinCondObl.eot' | file_url }}");
          src: url("{{ 'HelveticaNeue-ThinCondObl.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'HelveticaNeue-ThinCondObl.woff2' | file_url }}") format('woff2'),
              url("{{ 'HelveticaNeue-ThinCondObl.woff' | file_url }}") format('woff'),
              url("{{ 'HelveticaNeue-ThinCondObl.ttf' | file_url }}") format('truetype'),
              url("{{ 'HelveticaNeue-ThinCondObl.svg#HelveticaNeue-ThinCondObl' | file_url }}") format('svg');
          font-display: swap;
      }

      @font-face {
          font-family: 'Helvetica ThinItalic';
          src: url("{{ 'HelveticaNeue-ThinItalic.eot' | file_url }}");
          src: url("{{ 'HelveticaNeue-ThinItalic.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'HelveticaNeue-ThinItalic.woff2' | file_url }}") format('woff2'),
              url("{{ 'HelveticaNeue-ThinItalic.woff' | file_url }}") format('woff'),
              url("{{ 'HelveticaNeue-ThinItalic.ttf' | file_url }}") format('truetype'),
              url("{{ 'HelveticaNeue-ThinItalic.svg#HelveticaNeue-ThinItalic' | file_url }}") format('svg');
          font-display: swap;
      }

      @font-face {
          font-family: 'Helvetica ThinExtObl';
          src: url("{{ 'HelveticaNeue-ThinExtObl.eot' | file_url }}");
          src: url("{{ 'HelveticaNeue-ThinExtObl.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'HelveticaNeue-ThinExtObl.woff2' | file_url }}") format('woff2'),
              url("{{ 'HelveticaNeue-ThinExtObl.woff' | file_url }}") format('woff'),
              url("{{ 'HelveticaNeue-ThinExtObl.ttf' | file_url }}") format('truetype'),
              url("{{ 'HelveticaNeue-ThinExtObl.svg#HelveticaNeue-ThinExtObl' | file_url }}") format('svg');
          font-display: swap;
      }

      @font-face {
          font-family: 'Helvetica Thin';
          src: url("{{ 'HelveticaNeue-Thin.eot' | file_url }}");
          src: url("{{ 'HelveticaNeue-Thin.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'HelveticaNeue-Thin.woff2' | file_url }}") format('woff2'),
              url("{{ 'HelveticaNeue-Thin.woff' | file_url }}") format('woff'),
              url("{{ 'HelveticaNeue-Thin.ttf' | file_url }}") format('truetype'),
              url("{{ 'HelveticaNeue-Thin.svg#HelveticaNeue-Thin' | file_url }}") format('svg');
          font-display: swap;
      }

      @font-face {
          font-family: 'Helvetica ThinExt';
          src: url("{{ 'HelveticaNeue-ThinExt.eot' | file_url }}");
          src: url("{{ 'HelveticaNeue-ThinExt.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'HelveticaNeue-ThinExt.woff2' | file_url }}") format('woff2'),
              url("{{ 'HelveticaNeue-ThinExt.woff' | file_url }}") format('woff'),
              url("{{ 'HelveticaNeue-ThinExt.ttf' | file_url }}") format('truetype'),
              url("{{ 'HelveticaNeue-ThinExt.svg#HelveticaNeue-ThinExt' | file_url }}") format('svg');
          font-display: swap;
      }

      @font-face {
          font-family: 'Couture BoldItalic';
          src: url("{{ 'Couture-BoldItalic.eot' | file_url }}");
          src: url("{{ 'Couture-BoldItalic.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'Couture-BoldItalic.woff2' | file_url }}") format('woff2'),
              url("{{ 'Couture-BoldItalic.woff' | file_url }}") format('woff'),
              url("{{ 'Couture-BoldItalic.ttf' | file_url }}") format('truetype'),
              url("{{ 'Couture-BoldItalic.svg#Couture-BoldItalic' | file_url }}") format('svg');
          font-weight: bold;
          font-style: italic;
          font-display: swap;
      }

      @font-face {
          font-family: 'Couture Bold';
          src: url("{{ 'Couture-Bold.eot' | file_url }}");
          src: url("{{ 'Couture-Bold.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'Couture-Bold.woff2' | file_url }}") format('woff2'),
              url("{{ 'Couture-Bold.woff' | file_url }}") format('woff'),
              url("{{ 'Couture-Bold.ttf' | file_url }}") format('truetype'),
              url("{{ 'Couture-Bold.svg#Couture-Bold' | file_url }}") format('svg');
          font-weight: bold;
          font-style: normal;
          font-display: swap;
      }

      @font-face {
          font-family: 'Open Sans Semibold';
          src: url("{{ 'OpenSans-Semibold.eot' | file_url }}");
          src: url("{{ 'OpenSans-Semibold.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'OpenSans-Semibold.woff2' | file_url }}") format('woff2'),
              url("{{ 'OpenSans-Semibold.woff' | file_url }}") format('woff'),
              url("{{ 'OpenSans-Semibold.ttf' | file_url }}") format('truetype'),
              url("{{ 'OpenSans-Semibold.svg#OpenSans-Semibold' | file_url }}") format('svg');
          font-weight: 600;
          font-style: normal;
          font-display: swap;
      }

      @font-face {
          font-family: 'Open Sans';
          src: url("{{ 'OpenSans.eot' | file_url }}");
          src: url("{{ 'OpenSans.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'OpenSans.woff2' | file_url }}") format('woff2'),
              url("{{ 'OpenSans.woff' | file_url }}") format('woff'),
              url("{{ 'OpenSans.ttf' | file_url }}") format('truetype'),
              url("{{ 'OpenSans.svg#OpenSans' | file_url }}") format('svg');
          font-weight: normal;
          font-style: normal;
          font-display: swap;
      }

      @font-face {
          font-family: 'AXIS Extra';
          src: url("{{ 'AXIS-ExtraBold.eot' | file_url }}");
          src: url("{{ 'AXIS-ExtraBold.?#iefix' | file_url }}") format('embedded-opentype'),
              url("{{ 'AXIS-ExtraBold.woff2' | file_url }}") format('woff2'),
              url("{{ 'AXIS-ExtraBold.woff' | file_url }}") format('woff'),
              url("{{ 'AXIS-ExtraBold.ttf' | file_url }}") format('truetype'),
              url("{{ 'AXIS-ExtraBold.svg#AXIS-ExtraBold' | file_url }}") format('svg');
          font-weight: bold;
          font-style: normal;
          font-display: swap;
      }
    {% endstyle %}

    {{ 'base.css' | asset_url | stylesheet_tag }}
    {{ 'main.css' | asset_url | stylesheet_tag }}

    {%- if localization.available_countries.size > 1 or localization.available_languages.size > 1 -%}
      {{ 'component-localization-form.css' | asset_url | stylesheet_tag: preload: true }}
      <script src="{{ 'localization-form.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- if settings.predictive_search_enabled -%}
      <link
        rel="stylesheet"
        href="{{ 'component-predictive-search.css' | asset_url }}"
        media="print"
        onload="this.media='all'"
      >
    {%- endif -%}

    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
      if (Shopify.designMode) {
        document.documentElement.classList.add('shopify-design-mode');
      }
    </script>
  </head>

  <body class="pl-0 gradient{% if settings.animations_hover_elements != 'none' %} animate--hover-{{ settings.animations_hover_elements }}{% endif %}">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ 'accessibility.skip_to_text' | t }}
    </a>

    {%- if settings.cart_type == 'drawer' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}

    {% sections 'header-group' %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% sections 'footer-group' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>

    <script>
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}',
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`,
      };

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
        unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]' }}`,
      };

      window.quickOrderListStrings = {
        itemsAdded: `{{ 'sections.quick_order_list.items_added.other' | t: quantity: '[quantity]' }}`,
        itemAdded: `{{ 'sections.quick_order_list.items_added.one' | t: quantity: '[quantity]' }}`,
        itemsRemoved: `{{ 'sections.quick_order_list.items_removed.other' | t: quantity: '[quantity]' }}`,
        itemRemoved: `{{ 'sections.quick_order_list.items_removed.one' | t: quantity: '[quantity]' }}`,
        viewCart: `{{- 'sections.quick_order_list.view_cart' | t -}}`,
        each: `{{- 'sections.quick_order_list.each' | t: money: '[money]' }}`,
      };

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
        recipientFormExpanded: `{{ 'recipient.form.expanded' | t }}`,
        recipientFormCollapsed: `{{ 'recipient.form.collapsed' | t }}`,
        countrySelectorSearchCount: `{{ 'localization.country_results_count' | t: count: '[count]' }}`,
      };
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
  </body>
</html>
