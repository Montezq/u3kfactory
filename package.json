{
  "name": "u3faktory",
  "version": "1.0.0",
  "description": "U3Faktory shopify theme",
  "scripts": {
    "dev": "concurrently \"npm run watch:css\" \"npm run shopify:dev\"",
    "watch:css": "postcss styles/app.scss -o assets/main.css --watch",
    "shopify:dev": "shopify theme dev --live-reload full-page -e dev"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Montezq/u3kfactory"
  },
  "author": "Ion Talmaci",
  "license": "ISC"
}
